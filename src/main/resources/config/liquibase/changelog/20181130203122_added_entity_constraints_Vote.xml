<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Vote.
    -->
    <changeSet id="20181130203122-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="match_id"
                                 baseTableName="vote"
                                 constraintName="fk_vote_match_id"
                                 referencedColumnNames="id"
                                 referencedTableName="match"/>

        <addForeignKeyConstraint baseColumnNames="voter_id"
                                 baseTableName="vote"
                                 constraintName="fk_vote_voter_id"
                                 referencedColumnNames="id"
                                 referencedTableName="player"/>

    </changeSet>
</databaseChangeLog>
